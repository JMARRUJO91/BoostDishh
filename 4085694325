                mapDelete(idToReason, reasonId),
                cancelChecking && 0 === idToReason.size && (cancelChecking(),
                cancelChecking = void 0)
            }
              , promiseToReasonId = new WeakMap
              , promiseToReason = new FinalizationRegistry((heldReasonId=>{
